{
  "name": "NcLinkerAPIServer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "predev": "pnpm run swagger",
    "prebuild": "pnpm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsoa": "^5.1.1",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.3",
    "@types/swagger-ui-express": "^4.1.6",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
